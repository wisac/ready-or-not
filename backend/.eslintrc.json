{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "quotes": ["warn", "double"],
      "no-unused-vars": "warn",
      "no-underscore-dangle": "off",
      "indent": ["warn", 3],
      "no-console": "off",
      "semi": "warn",
      "import/no-extraneous-dependencies": "off",
      "import/extensions": "off"
    }
}
